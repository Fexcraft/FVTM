{
    "multipart": [
        {   "apply": { "model": "fvtm:constructor_cable_middle" }},
        {   "when": { "north": "true" },
            "apply": { "model": "fvtm:constructor_cable_north", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "fvtm:constructor_cable_north", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "fvtm:constructor_cable_south", "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "fvtm:constructor_cable_south", "y": 90, "uvlock": true }
        }
    ]
}